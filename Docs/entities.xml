<?xml version="1.0"?><!DOCTYPE entities SYSTEM "entities.dtd"><entities>    <entity name="Estate">		<table name="estates" autoincrement="true" />		<pk>			<field name="id" />		</pk>		<fields>			<field name="id" type="int" required="true" />			<field name="type" type="Type" required="true" />			<field name="photo" type="Photo" required="false" />			<field name="date" type="date" required="true" />			<field name="description" type="text" required="false" />			<field name="title" type="string" required="true" length="256" />			<field name="active" type="bool" required="false" />		</fields>	</entity>    <entity name="Machinery">		<table name="machineries" autoincrement="true" />		<pk>			<field name="id" />		</pk>		<fields>			<field name="id" type="int" required="true" />			<field name="photo" type="Photo" required="false" />			<field name="date" type="date" required="true" />			<field name="description" type="text" required="false" />			<field name="title" type="string" required="true" length="256" />			<field name="active" type="bool" required="false" />		</fields>	</entity>    <entity name="Partner">		<table name="partners" autoincrement="true" />		<pk>			<field name="id" />		</pk>		<fields>			<field name="id" type="int" required="true" />			<field name="photo" type="Photo" required="false" />			<field name="date" type="date" required="true" />			<field name="description" type="text" required="false" />			<field name="title" type="string" required="true" length="256" />			<field name="premium" type="bool" required="false" />			<field name="active" type="bool" required="false" />		</fields>	</entity>    <entity name="Photo">		<table name="photos" autoincrement="true" />		<pk>			<field name="id" />		</pk>		<fields>			<field name="id" type="int" required="true" />			<field name="alt" type="string" required="false" length="256" />			<field name="thumbnail" type="string" required="true" length="256" />			<field name="url" type="string" required="true" length="256" />		</fields>	</entity>    <entity name="Type">		<table name="types" autoincrement="true" />		<pk>			<field name="id" />		</pk>		<fields>			<field name="id" type="int" required="true" />			<field name="name" type="string" required="true" length="256" />		</fields>	</entity>    <entity name="Role">		<table name="roles" autoincrement="true" />		<pk>			<field name="id" />		</pk>		<fields>			<field name="id" type="int" required="true" />			<field name="name" type="string" required="true" length="256" />		</fields>	</entity>    <entity name="User">		<table name="users" autoincrement="false" />		<pk>			<field name="id" />		</pk>		<fields>			<field name="id" type="string" required="true" length="32" />			<field name="password" type="string" required="true" length="32" />			<field name="name" type="string" required="true" length="256" />			<field name="lastActionDate" type="date" required="true" />			<field name="role" type="Role" required="true" />			<field name="active" type="bool" required="false" />		</fields>	</entity></entities>