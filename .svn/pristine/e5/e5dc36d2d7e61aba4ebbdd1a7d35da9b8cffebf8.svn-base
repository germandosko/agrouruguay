/*see http://css3generator.com/*/
html, body {width:100%; height:100%;}
body{font-family:Verdana, Geneva, sans-serif; font-size:16px; line-height:1.3em; color:#161; background-color:#fff;}
a {color:#000; text-decoration:none;}
a:hover {text-decoration:none;}

.clearfix, .clearfix:after {float:none; clear:both;}
.clearfix:before, .clearfix:after {content:" "; display:block; height:0; visibility:hidden;}
.bold{font-weight:900;}

form{position:relative; width:85%; min-width:300px; padding:1%; margin:0px auto;}
	form fieldset{position:relative; padding:15px; text-align:center;}
	form fieldset.ui-corner-all{background-color:#d8d8d8; border:1px solid #eee;}
		form label{font-weight:bold; color:#459E00; margin-bottom:5px;}
		form label, form input[type=text], form input[type=password], form textarea{display:block; position:relative; width:100%; min-width:300px; text-align:left;}
		form input[type=submit], form input[type=reset], form label{margin-top:10px;}
		form textarea{height:75px;}
		form a.exit{position:absolute; bottom:10px; left:30px;}
		form p.error, form p.warning, form p.success{position:relative; padding:0 0 0 5px; margin:0; text-align:center; vertical-align:middle; font:italic 100 14px sans-serif;}
		form p.error{color:#f00;}
		form p.warning{color:#f60;}
		form p.success{color:#090;}
			form p.error img, form p.warning img, form p.success img{width:16px; height:16px; margin-right:0.5em;}

table{position:relative; width:100%; padding:0; margin:0; text-align:center;}
	table tr{border-top:1px solid #bbb;}
	table tr:last-child{border:none;}
		table tr th{font:normal 900 18px serif; padding:0;}
		table tr td{font:normal 100 18px serif; padding:5px; vertical-align:middle;}
		table tr td.text{text-align:left;}
		table tr td.options{border:none; min-width:70px;}
			table tr td a:hover{cursor:pointer; text-decoration:none;}

#header {position:relative; width:100%; min-height:180px; margin:0px auto 0px auto; background:url("../img/bg.png") repeat-x bottom left; }
	#tool-bar {position:relative; left:0; padding:10px 0 0 5%; width:auto; float:left;}
		#tool-bar img{margin-left:10px; vertical-align:middle;}
		#searchIcon{cursor:pointer;}
		#tool-bar input{vertical-align:middle;}
	#nav-bar {position:relative; right:0; padding:10px 5% 0 0; width:auto; float:right;}
		#nav-bar li {position:relative; float:left; margin-left:25px; font-family:'Shadows Into Light Two', cursive, serif; font-size:24px; color:#000;}
			#nav-bar li a{font-family:'Shadows Into Light Two', cursive, serif; font-size:24px; color:#000;}
			#nav-bar li a:hover{color:#0D0;}
			#nav-bar li a.active{color:#191;}
			#nav-bar li a.active:hover{color:#191; cursor:default;}
	#promo-images{position:relative; padding:0 30px; float:left; top:30px; left:5%;}
		#promo-images img {position:relative; display:block; margin:6px; float:left; -webkit-box-shadow:0px 0px 5px 0px; box-shadow:0px 0px 5px 0px;}
		#promo-images a.prev, #promo-images a.next {position:absolute; display:block; width:28px; height:40px; top:30px; background:url(../img/arrows.png) no-repeat transparent;}		
		#promo-images a.next {right:0; background-position:0 0;}
		#promo-images a.next:hover {background-position:-28px 0;}
		#promo-images a.prev {left:0; background-position:-84px 0;}
		#promo-images a.prev:hover {background-position:-56px 0;}
		#promo-images a.prev span, #promo-images a.next span {display:none;}
	#logo{position:relative; width:auto; top:30px; right:5%; float:right;}
	
#content {position:relative; width:100%; min-height:300px; margin:0px auto 0px auto; padding-bottom:70px;}
	#main {position:relative; width:90%; padding:10px 5%; min-height:300px;}
		#main div.left, #main div.right{position:relative; width:48%; float:left; padding:0; margin:0 1%; min-height:300px; text-align:center;}
			#main div.left p, #main div.right p {display:block; position:relative; margin:10px 0; text-align:left;}
			#main div.left ul li, #main div.right ul li {display:block;position:relative; margin:15px 0; -webkit-box-shadow:0px 0px 5px 0px; box-shadow:0px 0px 5px 0px;}
				#main div.left ul li img.info, #main div.right ul li img.info {position:relative; display:block; float:left; width:25%; margin:2% 5% 2% 2%; -webkit-box-shadow:0px 0px 5px 0px; box-shadow:0px 0px 5px 0px;}
				#main div.left ul li img.logo, #main div.right ul li img.logo {position:relative; display:block; float:left; width:15%; margin:2% 5% 2% 2%; -webkit-box-shadow:0px 0px 5px 0px; box-shadow:0px 0px 5px 0px;}
					#main div.left ul li div.info, #main div.right ul li div.info {position:relative; float:left; width:60%; margin:2%;}
						#main div.left ul li div.info h3, #main div.right ul li div.info h3{font-weight:bold; color:#459E00; margin-bottom:1%; text-align:left;}
				#main div.left ul li div.sale-rent, #main div.right ul li div.sale-rent{position:absolute; right:-2px; bottom:-2px; width:80px; height:80px;}
					#main div.left ul li div.sale-rent img, #main div.right ul li div.sale-rent  img{width:100%; height:100%;}
		#main div.left div.contact{padding:9px; margin:0 auto 0 auto; color:#000;}
			#main div.left div.contact h2 {text-align:center; font-weight:bold; color:#459E00;}
			#main div.left div.contact p{padding:0px; margin:0px auto 0px auto; line-height:1.3em; text-align:center; text-indent:0em;}
				#main div.left div.contact p span.info{display:block; font:italic; text-align:center;}
				
			#main-info {display:block; width:80%; margin:80px auto; font-weight:400; text-align:left;}
			#origianl-logo {display:block; width:auto; margin:30px auto; text-align:center;}
				#origianl-logo img{ -webkit-box-shadow:0px 0px 5px 0px; box-shadow:0px 0px 5px 0px;}
				
	#login {position:relative; width:45%; min-width:300px; margin:0 auto;}
	
	#system {position:relative; width:98%; min-width:320px; margin:1% auto 0 auto;}
	
	#oldPhoto {position:relative; margin:2% auto 1% auto; text-align:center;}
		#oldPhoto img{width:200px; border:1px solid #666;}
		#oldPhoto p.error, #oldPhoto p.warning, #oldPhoto p.success{position:relative; margin:10px; font-weight:900;}
			#oldPhoto p.error img, #oldPhoto p.warning img, #oldPhoto p.success img{width:16px; height:16px; border:none;}
	#jCropTarget{display:block; position:relative; margin:0px auto 0px auto; border:1px solid #666; max-width:950px;}
		

	#lock {position:relative; width:90px; margin:30px auto 10px auto;}
	#progress {position:relative; width:500px; margin:0px auto 0px auto; text-align:center; font-family:'Shadows Into Light Two', cursive, serif; font-size:24px; color:#191;}
		#progress h1{position:relative; margin:0px auto 20px auto; text-align:center; font-family:'Shadows Into Light Two', cursive, serif; font-size:30px; color:#191;}
		#progress span{ color:#666;}
		#progressbar{font-size:12px;}

#footer {position:fixed; left:0px; bottom:0px; width:100%; height:104px; background:url("../img/grass-patern.png") repeat-x bottom left;}